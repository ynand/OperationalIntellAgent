        {
            cmd.CommandText = selectSql;
            for (var i = 0; i < ids.Count; i++)
            {
                cmd.Parameters.AddWithValue($"@id{i}", ids[i]);
            }

            using var reader = cmd.ExecuteReader();
            var results = new List<object>();
            while (reader.Read())
            {
                results.Add(new {
                    Id = reader.GetInt32(0),
                    Value = reader.GetInt32(1)
                });
            }
            return Ok(results);
        }
    }
    catch (Exception ex)
