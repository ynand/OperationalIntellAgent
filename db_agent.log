[HttpPost("run-buggy")]
public IActionResult RunBuggy()
{
    // Initialize list of parameters for batch processing
    var ids = new List<int>();
    using (var cmd = _conn.CreateCommand())
    {
        cmd.CommandText = "SELECT Id FROM BuggyTable";
        using var reader = cmd.ExecuteReader();
        while (reader.Read())
        {
            ids.Add(reader.GetInt32(0));
        }
    }
    
    // Using parameterized queries for updates and selects
    var idParameters = new List<string>();
    for (var i = 0; i < ids.Count; i++)
    {
        idParameters.Add($"@id{i}");
    }

    var selectSql = $"SELECT * FROM BuggyTable WHERE Id IN ({string.Join(",", idParameters)})";
    var updateSql = $"UPDATE BuggyTable SET Value = Value + 1 WHERE Id IN ({string.Join(",", idParameters)})";

    try
    {
