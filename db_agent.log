{
    cmd.CommandText = "UPDATE BuggyTable SET Value = Value + 1 WHERE Id = @Id";
    var idParam = cmd.CreateParameter();
    cmd.Parameters.Add(idParam);

    foreach (var id in ids)
    {
        idParam.Value = id;
        cmd.ExecuteNonQuery();
    }
}

// Fetching results
using (var cmd = _conn.CreateCommand())
{
    cmd.CommandText = "SELECT Id, Value FROM BuggyTable WHERE Id = @Id";
    var idParam = cmd.CreateParameter();
    cmd.Parameters.Add(idParam);

    var results = new List<object>();
    foreach (var id in ids)
    {
        idParam.Value = id;
        using var reader = cmd.ExecuteReader();
        while (reader.Read())
        {
            results.Add(new {
                Id = reader.GetInt32(0),
                Value = reader.GetInt32(1)
            });
        }
    }
    return Ok(results);
}
