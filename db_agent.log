    if (ids.Count == 0)
    {
        return Ok(new List<object>());
    }
    
    using (var transaction = _conn.BeginTransaction())
    {
        string tempTableSql = "CREATE TABLE #TempIds (Id INT);";
        using (var cmd = _conn.CreateCommand())
        {
